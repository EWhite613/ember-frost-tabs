<div class="tab-nav">
  {{#each tabs as |tab|}}
    {{component tab hook=hook targetOutlet=targetOutlet selectedTab=selectedTab onChange=onChange}}
  {{/each}}
</div>
<div class='content' data-test={{hook (concat hook 'content')}}>
  {{#from-elsewhere name=targetOutlet as |tab|}}
    {{component tab.content}}
  {{/from-elsewhere}}
</div>
